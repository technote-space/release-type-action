    "@technote-space/github-action-version-helper": "^0.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@technote-space/github-action-test-helper": "^0.6.4",
    "@technote-space/release-github-actions-cli": "^1.7.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "^10.5.2",
    "nock": "^13.0.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
